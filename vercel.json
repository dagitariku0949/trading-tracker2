{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install",
  "framework": "vite",
  "functions": {
    "api/trades.js": {
      "runtime": "nodejs18.x"
    },
    "api/learning.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/trades/(.*)",
      "destination": "/api/trades?path=$1"
    },
    {
      "source": "/api/trades",
      "destination": "/api/trades"
    },
    {
      "source": "/api/learning/(.*)",
      "destination": "/api/learning?path=$1"
    },
    {
      "source": "/api/learning",
      "destination": "/api/learning"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
