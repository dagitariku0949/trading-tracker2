{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install",
  "framework": "vite",
  "functions": {
    "api/learning.js": {
      "runtime": "nodejs18.x"
    },
    "api/auth.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/learning",
      "destination": "/api/learning.js"
    },
    {
      "source": "/api/auth",
      "destination": "/api/auth.js"
    },
    {
      "source": "/admin-panel",
      "destination": "/admin-panel.html"
    },
    {
      "source": "/admin",
      "destination": "/admin.html"
    },
    {
      "source": "/admin.html",
      "destination": "/admin.html"
    },
    {
      "source": "/admin-panel.html",
      "destination": "/admin-panel.html"
    },
    {
      "source": "/api-test",
      "destination": "/api-test.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
